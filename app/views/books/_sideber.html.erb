        <h1>New book</h1>
          <%= form_with model: book do |f| %>
            <div class="field">
              <%= f.label :title %><br>
              <%= f.text_field :title %>
            </div>
            <div class="field">
              <%= f.label :body %><br>
              <%= f.text_area :body %>
            </div>
            <%= f.submit 'Create Book' %>
          <% end %>
      </div>